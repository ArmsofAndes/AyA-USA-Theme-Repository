            "video": "https://www.youtube.com/watch?v=_9VUPq3SxOc",
            "video_hosted": "shopify://files/videos/14.mp4"
          }
        },
        "video_full_k4M3jm": {
          "type": "video_full",
          "settings": {
            "video_full": "https://www.youtube.com/watch?v=_9VUPq3SxOc",
            "video_hosted_full": "shopify://files/videos/17 II_4002f204-9377-468e-957d-e5194649cf10.mp4"
