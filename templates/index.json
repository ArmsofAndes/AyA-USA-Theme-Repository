    "custom_liquid_jzQjfa": {
      "type": "custom-liquid",
      "name": "Custom liquid",
      "settings": {
        "custom_liquid": "<script>\n(function run() {\n  function italicize(container) {\n    container.querySelectorAll('.scrolling-text--item span').forEach(span => {\n      const txt = (span.textContent || '').trim();\n      if (!txt.includes('-')) return;\n      const parts = txt.split('-');\n      const before = parts.shift().trim();\n      const after = parts.join('-').trim();\n      span.innerHTML = before ? (before + ' - <i>' + after + '</i>') : ('<i>' + after + '</i>');\n    });\n  }\n\n  // Ejecuta en carga\n  const containers = document.querySelectorAll('.section-scrolling-text, .scrolling-text');\n  containers.forEach(italicize);\n\n  // Re-ejecuta cuando Shopify recarga secciones en el editor\n  document.addEventListener('shopify:section:load', (e) => {\n    const el = e.target;\n    if (el && (el.classList.contains('section-scrolling-text') || el.querySelector('.scrolling-text'))) {\n      italicize(el);\n    }\n  });\n})();\n</script>",
        "section_width": "full-width-row"
      }
    },
