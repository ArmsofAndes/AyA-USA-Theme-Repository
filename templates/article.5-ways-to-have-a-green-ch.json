    "rich_text_iPyQYA": {
      "type": "rich-text",
      "blocks": {
        "text_8cH3ee": {
          "type": "text",
          "settings": {
            "text": "<p>Globally, during Christmas, our waste levels increase by almost 30% with excess packaging, wrapping paper, holiday cards, food waste, and unsustainable practices. </p>",
            "custom_liquid": "",
            "color": "#000000",
            "font_size": 25,
            "font_weight": 300
          }
        }
      },
      "block_order": [
        "text_8cH3ee"
      ],
      "custom_css": [
        ".richtext-full-bg {margin: 0 10% 0 10%;}",
        ".rte {line-height: 2em; letter-spacing: 0.5px; text-align: justify;}"
      ],
      "name": "Rich text",
      "settings": {
        "section_width": "full-width-row",
        "text_width": "grid",
        "text_position": "left",
